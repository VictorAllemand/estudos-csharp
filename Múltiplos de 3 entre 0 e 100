using System;

public class Program
{
    public static void Main(string[] args)
    {
        Console.WriteLine("Múltiplos de 3 entre 0 e 100:");

        // Loop de 0 a 100 para verificar cada número
        for (int i = 0; i <= 100; i++)
        {
            // Verifica se o número é múltiplo de 3
            if (i % 3 == 0)
            {
                Console.WriteLine(i); // Mostra o número se for múltiplo de 3
            }
        }

        Console.ReadLine(); // Aguarda o usuário pressionar Enter para sair
    }
}
